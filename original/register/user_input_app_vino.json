{
    "services": [
    {
	"org": "$HZN_ORG_ID",
	"url": "$EDGE_OWNER.$EDGE_DEPLOY.http",
	"versionRange": "[0.0.0,INFINITY)",
	"variables": {
	    "APP_APIKEY_PASSWORD"                : "$APP_APIKEY_PASSWORD",
	    "APP_IEAM_API_CSS_OBJECTS"           : "$APP_IEAM_API_CSS_OBJECTS",
	    "APP_MMS_OBJECT_SERVICE_NAME_CONFIG" : "$APP_MMS_OBJECT_SERVICE_NAME_CONFIG",
	    "APP_VIEW_COLUMNS"                   : $APP_VIEW_COLUMNS,
            "EVENTSTREAMS_BROKER_URLS"           : "$EVENTSTREAMS_BROKER_URLS",
            "EVENTSTREAMS_API_KEY"               : "$EVENTSTREAMS_API_KEY",
            "EVENTSTREAMS_ENHANCED_TOPIC"        : "$EVENTSTREAMS_ENHANCED_TOPIC"
	}
    },
    {
	"org": "$HZN_ORG_ID",
	"url": "$EDGE_OWNER.$EDGE_DEPLOY.mmsmodel",
	"versionRange": "[0.0.0,INFINITY)",
	"variables": {
	    "APP_BIND_HORIZON_DIR"     : "$APP_BIND_HORIZON_DIR"
	}
    },
    {
	"org": "$HZN_ORG_ID",
	"url": "$EDGE_OWNER.$EDGE_DEPLOY.vino",
	"versionRange": "[0.0.0,INFINITY)",
	"variables": {
	    "APP_MODEL_FMWK"           : "$APP_MODEL_FMWK",
	    "APP_CAMERAS"              : "$APP_CAMERAS",
	    "APP_VIDEO_FILES"          : "$APP_VIDEO_FILES",
	    "APP_RTSPS"                : "$APP_RTSPS",
	    "APP_VIEW_COLUMNS"         : $APP_VIEW_COLUMNS,
            "DEVICE_ID"                : "$DEVICE_ID",
            "DEVICE_NAME"              : "$DEVICE_NAME",
	    "DEVICE_IP_ADDRESS"        : "$DEVICE_IP_ADDRESS",
	    "APP_BIND_HORIZON_DIR"     : "$APP_BIND_HORIZON_DIR",
	    "SHOW_OVERLAY"             : $SHOW_OVERLAY,
	    "PUBLISH_KAFKA"            : $PUBLISH_KAFKA,
	    "PUBLISH_STREAM"           : $PUBLISH_STREAM,
	    "DETECT_FACE"              : $DETECT_FACE,
	    "BLUR_FACE"                : $BLUR_FACE,
	    "MIN_CONFIDENCE_THRESHOLD" : 0.6
	}
    }
  ]
}
